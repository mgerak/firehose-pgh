<h2>
	<% @pics.each do |pic| %>
		<div class="stolen-frosty span7 text-center">
			<h1 class="stolen-h1">
			Lesson: <%=pic.lesson %>
			</h1>
			
				<%= link_to pic_path (pic) do %>
				<%= image_tag pic.image %>
			<% end %>

		
			<h2>
			Emotion: <%= pic.emotion %>
			</h2>
		
			
			<h2>
			Learned: <%= pic.learned %>
			</h2>
		</div>
	<% end %>
</h2>