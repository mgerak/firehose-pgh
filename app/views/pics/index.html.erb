<h2>
	<% @pics.each do |pic| %>
		<div class="stolen-frosty span7 text-center">
			<h1 class="stolen-h1">
			Lesson: <%=pic.lesson %>
			</h1>
			
			<%= image_tag pic.image %>

			<br/>	
			<h2>
			Emotion: <%= pic.emotion %>
			</h2>
			<br/>
			
			<h2>
			Learned: <%= pic.learned %>
			</h2>
		</div>
	<% end %>
</h2>